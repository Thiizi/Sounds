CMD:sound(playerid, params[])
{
	new sound = strval(params);
	if(sound <= 0 || sound > 231) return SendClientMessage(playerid, 0xA9C4E4FF, "/sound <0-231>");
	PlayerPlaySound(playerid, SAMP_Sounds[sound][soundid], 0.0, 0.0, 0.0);

	new str[54];
	SendClientMessage(playerid, 0x88AA62FF, "");
	format(str, sizeof str, "Sound: %s(%d)", SAMP_Sounds[sound][Sound], SAMP_Sounds[sound][soundid]);
	SendClientMessage(playerid, 0x88AA62FF, str);

	format(str, sizeof str, "PlayerPlaySound(playerid, %d, 0.0, 0.0, 0.0);", SAMP_Sounds[sound][soundid]);
	SendClientMessage(playerid, 0x88AA62FF, str);
	return 1;
}